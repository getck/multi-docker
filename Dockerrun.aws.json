{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "chida1234/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name":"server",
            "image": "chida1234/multi-server",
            "hostname": "api",
            "essential":false
        },
        {
            "name":"worker",
            "image": "chida1234/multi-worker",
            "hostname": "worker",
            "essential":false
        },
        {
            "name": "ngnix",
            "image": "chida1234/multi-nginx",
            "hostname" : "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostport":80,
                    "containedPort:'80'

                }
            ],
            
        }
    ]
}